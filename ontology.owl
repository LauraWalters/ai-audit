    <!-- https://w3id.org/auditops#AuditBundle -->


    <owl:Class rdf:about="https://w3id.org/auditops#AuditBundle">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Bundle"/>
        <rdfs:comment xml:lang="en">Container for a single execution run (e.g., one workflow run). Typically aligned with prov:Bundle in runtime traces.</rdfs:comment>
        <rdfs:label xml:lang="en">Audit Bundle</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/auditops#AuditableAISystem -->


    <owl:Class rdf:about="https://w3id.org/auditops#AuditableAISystem">
        <rdfs:comment xml:lang="en">A software system (or deployed AI service) for which audit evidence (traces) is collected.</rdfs:comment>
        <rdfs:label xml:lang="en">Auditable AI System</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/auditops#Confidence -->


    <owl:Class rdf:about="https://w3id.org/auditops#Confidence">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/mls#ModelEvaluation"/>
        <rdfs:comment xml:lang="en">A quality node (typically linked via mls:hasQuality) holding confidence values.</rdfs:comment>
        <rdfs:label xml:lang="en">Confidence</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/auditops#DataInstance -->


    <owl:Class rdf:about="https://w3id.org/auditops#DataInstance">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:comment xml:lang="en">A data artifact used/produced by workflow steps (can be a single document, batch, or dataset).</rdfs:comment>
        <rdfs:label xml:lang="en">Data Instance</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/auditops#Dataset -->


    <owl:Class rdf:about="https://w3id.org/auditops#Dataset">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/mls#Dataset"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/auditops#DataInstance"/>
        <rdfs:comment xml:lang="en">A dataset (typically multi-row or multi-item collection) used for training/evaluation.</rdfs:comment>
        <rdfs:label xml:lang="en">Dataset</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/auditops#Engineering -->


    <owl:Class rdf:about="https://w3id.org/auditops#Engineering">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Activity"/>
        <rdfs:comment xml:lang="en">Non-learning engineering activity (e.g., KG/ontology/rules/software engineering).</rdfs:comment>
        <rdfs:label xml:lang="en">Engineering Activity</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/auditops#ExecutionStatus -->


    <owl:Class rdf:about="https://w3id.org/auditops#ExecutionStatus">
        <rdfs:label xml:lang="en">Execution status</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/auditops#Inference -->


    <owl:Class rdf:about="https://w3id.org/auditops#Inference">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Activity"/>
        <rdfs:comment xml:lang="en">Activity producing outputs/predictions from a model given input data.</rdfs:comment>
        <rdfs:label xml:lang="en">Inference Activity</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/auditops#Model -->


    <owl:Class rdf:about="https://w3id.org/auditops#Model">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:comment xml:lang="en">A model artifact used for inference or produced by training.</rdfs:comment>
        <rdfs:label xml:lang="en">Model</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/auditops#ResultData -->


    <owl:Class rdf:about="https://w3id.org/auditops#ResultData">
        <rdfs:subClassOf rdf:resource="https://w3id.org/auditops#DataInstance"/>
        <rdfs:comment xml:lang="en">A collection of result elements (e.g., all predictions for one input document).</rdfs:comment>
        <rdfs:label xml:lang="en">Result Data</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/auditops#ResultElement -->


    <owl:Class rdf:about="https://w3id.org/auditops#ResultElement">
        <rdfs:subClassOf rdf:resource="https://w3id.org/auditops#DataInstance"/>
        <rdfs:comment xml:lang="en">A granular output item relevant for auditing (e.g., a predicted entity, a row-level decision, a ranked hit).</rdfs:comment>
        <rdfs:label xml:lang="en">Result Element</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/auditops#Software -->


    <owl:Class rdf:about="https://w3id.org/auditops#Software">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:comment xml:lang="en">Software component used in or produced by a workflow step.</rdfs:comment>
        <rdfs:label xml:lang="en">Software</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/auditops#StatisticalModel -->


    <owl:Class rdf:about="https://w3id.org/auditops#StatisticalModel">
        <rdfs:subClassOf rdf:resource="https://w3id.org/auditops#Model"/>
        <rdfs:comment xml:lang="en">A statistical/ML model (e.g., neural network) used for training/inference.</rdfs:comment>
        <rdfs:label xml:lang="en">Statistical Model</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/auditops#SymbolicModel -->


    <owl:Class rdf:about="https://w3id.org/auditops#SymbolicModel">
        <rdfs:subClassOf rdf:resource="https://w3id.org/auditops#Model"/>
        <rdfs:comment xml:lang="en">A non-statistical model (e.g., rules, KG, ontology).</rdfs:comment>
        <rdfs:label xml:lang="en">Symbolic Model</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/auditops#Training -->


    <owl:Class rdf:about="https://w3id.org/auditops#Training">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Activity"/>
        <rdfs:comment xml:lang="en">Learning activity that fits model parameters (training/fine-tuning).</rdfs:comment>
        <rdfs:label xml:lang="en">Training Activity</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/auditops#Transformation -->


    <owl:Class rdf:about="https://w3id.org/auditops#Transformation">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Activity"/>
        <rdfs:comment xml:lang="en">Deterministic data transformation without learning (e.g., parsing, normalization).</rdfs:comment>
        <rdfs:label xml:lang="en">Transformation Activity</rdfs:label>
    </owl:Class>
