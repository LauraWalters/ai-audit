    <owl:Ontology rdf:about="https://w3id.org/ai-audit/infrastructure#">
        <terms:abstract xml:lang="en">This ontology describes the infrastructure that supports the execution and management of audits, including systems, services, data sources, and artifacts. </terms:abstract>
        <terms:contributor>Marta Sabou</terms:contributor>
        <terms:contributor>Tomasz Miksa</terms:contributor>
        <terms:creator>Fajar J. Ekaputra</terms:creator>
        <terms:creator>Gregor Käfer, Laura Waltersdorfer</terms:creator>
        <terms:description xml:lang="en">Vocabulary for configuring auditability infrastructure: services, receive/query endpoints, mappings, and validation/transformation functions. Links endpoints to workflow steps.</terms:description>
        <terms:description>An ontology to support AI auditability</terms:description>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <terms:title xml:lang="en">AuditOps – Audit Infrastructure Module</terms:title>
        <owl:versionInfo>1.0.0</owl:versionInfo>
        <vocab:introduction xml:lang="en">This section introduces the audit infrastructure ontology and its scope. </vocab:introduction>
    </owl:Ontology>
    


