    <owl:Ontology rdf:about="https://w3id.org/ai-audit#">
        <terms:abstract xml:lang="en">This ontology describes concepts for AI workflows to model the sequence of activities, roles, and dependencies involved for conducting audits. It supports the representation and comparison of different workflow processes.</terms:abstract>
        <terms:contributor>Fajar J. Ekaputra</terms:contributor>
        <terms:contributor>Gregor Käfer</terms:contributor>
        <terms:contributor>Marta Sabou</terms:contributor>
        <terms:contributor>Tomasz Miksa</terms:contributor>
        <terms:creator>Laura Waltersdorfer</terms:creator>
        <terms:description xml:lang="en">Core vocabulary for representing auditable AI system workflows and runtime audit evidence (activities, entities, bundles). Reuses PROV-O, P-Plan.</terms:description>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <terms:title xml:lang="en">AuditOps – Workflow Module</terms:title>
        <vann:preferredNamespacePrefix>audit</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri rdf:resource="https://w3id.org/ai-audit/"/>
        <owl:versionInfo>1.0.0</owl:versionInfo>
        <vocab:introduction xml:lang="en">This section introduces the audit workflow ontology and its scope. </vocab:introduction>
    </owl:Ontology>
    


